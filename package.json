{
  "name": "sunbeam-weather-app",
  "productName": "Sunbeam-Weather-App",
  "version": "0.0.0",
  "license": "MIT",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron": "ng build --dev && electron ./dist/index.html",
    "electron-build": "ng build --prod && electron ./dist/index.html",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/SWA.icns --prune=true --out=release-builds",
    "package-win": "electron-packager .  Sunbeam-Weather-App --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/SWA.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Sunbeam Weather App\"",
    "package-linux": "electron-packager . Sunbeam-Weather-App --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/SWA.png --prune=true --out=release-builds"
  },
  "private": true,
  "dependencies": {
    "@agm/core": "^1.0.0-beta.2",
    "@angular/animations": "^5.2.10",
    "@angular/cdk": "^5.2.5",
    "@angular/common": "^5.2.10",
    "@angular/compiler": "^5.2.10",
    "@angular/core": "^5.2.10",
    "@angular/forms": "^5.2.10",
    "@angular/http": "^5.2.10",
    "@angular/material": "^5.2.5",
    "@angular/platform-browser": "^5.2.10",
    "@angular/platform-browser-dynamic": "^5.2.10",
    "@angular/router": "^5.2.10",
    "angular2-chartjs": "^0.5.0",
    "chart.js": "^2.7.2",
    "core-js": "^2.5.6",
    "hammerjs": "^2.0.8",
    "moment": "^2.22.1",
    "ngx-progressbar": "^2.1.1",
    "rxjs": "^5.5.10",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "~1.7.4",
    "@angular/compiler-cli": "^5.2.10",
    "@angular/language-service": "^5.2.10",
    "@types/jasmine": "^2.8.7",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^6.0.109",
    "codelyzer": "^4.3.0",
    "electron": "^1.8.6",
    "electron-packager": "^12.0.2",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^2.0.2",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "~2.5.3"
  }
}
